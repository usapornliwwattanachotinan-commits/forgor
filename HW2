        "**Plotting Multiple Lines**"
      ],
      "metadata": {
        "id": "AwvFERDFpRfn"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "import matplotlib.pyplot as plt\n",
        "\n",
        "years = range(2010, 2022)\n",
        "apples = [0.895, 0.91, 0.919, 0.926, 0.929, 0.931, 0.934, 0.936, 0.937, 0.9375, 0.9372, 0.939]\n",
        "oranges = [0.962, 0.941, 0.930, 0.923, 0.918, 0.908, 0.907, 0.904, 0.901, 0.898, 0.9, 0.896, ]\n",
        "\n",
        "plt.plot(years, apples, marker='o')\n",
        "plt.plot(years, oranges, marker='x')\n",
        "plt.xlabel('Year')\n",
        "plt.ylabel('Yield (tons per hectare)');\n",
        "plt.title(\"Crop Yields in Kanto\")\n",
        "plt.legend(['Apples', 'Oranges']);"
      ],
